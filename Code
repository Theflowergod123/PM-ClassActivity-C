class Main {
  public static void main(String[] args) {
    int sets=0;
    for(int n=0; n<=7; n++) {
      if (n+(n+1)+(n+2)+(n+3)+(n+4)==15){
        sets = sets+1;
      }
      else if (n+(n+1)+(n+2)+(n+3)==15){
        sets=sets+1;
      }
      else if (n+(n+1)+(n+2)==15){
        sets=sets+1;
      }
      else if (n+(n+1)==15) {
        sets=sets+1;
      } 
    }
    System.out.println(sets);
  }
}
