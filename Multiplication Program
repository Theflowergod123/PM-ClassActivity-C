class Main {

  public static void main(String[] args){

    int x = 8; // define variables
    int y = 22;
    int product = x*y;

    //displays question
    System.out.println("What is " + x + " multiplied by " + y + "?");
    //Changed output to display variables so program is useful in more than one senario
    //displays answer
    System.out.println(product + " This is the product.");

  }
}
